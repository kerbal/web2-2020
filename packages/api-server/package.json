{
  "name": "api-server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">12.0.0"
  },
  "scripts": {
    "db:seed": "sequelize-cli db:seed:undo:all && sequelize-cli db:seed:all",
    "db:seed:new": "sequelize-cli seed:generate --name",
    "db:migrate": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "dev": "nodemon index.js --ext js, ejs --exec babel-node",
    "start": "nodemon index.js --ext js, ejs --exec babel-node"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@babel/register": "^7.9.0",
    "babel-cli": "^6.26.0",
    "babel-node": "^0.0.1-security",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "nodemailer-markdown": "^1.0.3",
    "nodemon": "^2.0.3",
    "pg": "^8.1.0",
    "redis": "^3.0.2",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
